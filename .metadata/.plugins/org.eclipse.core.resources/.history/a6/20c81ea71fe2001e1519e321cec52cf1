package com.sathya.springex;

public class Person {
     String name;
     String email;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
     
     public void person() {
    	 System.out.println("Heyy"+name+"welcome to sathya Technologies");
    	 System.out.println("Your... email Id"+email+"will recieve updates.......");
     }
}
